    "type": "library",
    "license": "GPL-3.0+",
    "keywords": ["csv", "sqlite", "redis"],
    "homepage": "http://github.com/izap/Importer",
    "support": {
        "issues": "https://github.com/izap/Importer/issues"
    },
    "authors": [
        {
            "name": "Tarun Jangra",
            "email": "tarun@izap.in",
            "role": "lead",
            "homepage": "http://tarunjangra.com"
        }
    ],
    "require": {
        "php": ">=5.4.30",
        "ext-xdebug": ">=2.2.5",
        "ext-curl": "*",
        "ext-sqlite3": "*",
        "izap/spreadsheet-reader": "dev-master"
    },
    "require-dev":{
