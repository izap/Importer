{
    "name": "izap/importer",
    "description": "Distributed csv,xls importer in database (DID)",
    "type": "library",
    "license": "GPL-3.0+",
    "keywords": ["csv", "sqlite", "redis"],
    "homepage": "http://github.com/izap/importer",
    "support": {
        "issues": "https://github.com/izap/importer/issues"
    },
    "authors": [
        {
            "name": "Tarun Jangra",
            "email": "tarun@izap.in",
            "role": "lead",
            "homepage": "http://tarunjangra.com"
        }
    ],
    "require": {
      "php": ">=5.3.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/izap/importer.git"
        }
    ],
    "suggest": {
      "ext-curl": "PHP Curl",
      "ext-pthreads": "Workers ( PHP compile configuration)",
      "ext-gearman": "Gearman worker",
      "ext-sqlite": "PHP Sqlite3"
    },
    "require-dev":{
      "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
      "psr-0": {
        "Importer": "src/",
        "Importer\\Tests" : "tests/"
      }
    }
}
